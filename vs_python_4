def multiply1():
    counter1 = 1
    while counter1 <= 10:
        print(1*counter1)
        counter1 += 1
    counter2 = 1
    while counter2 <= 10:
        print(2*counter2)
        counter2 += 1
        counter3 = 1
    while counter3 <= 10:
        print(3*counter3)
        counter3 += 1
    counter4 = 1
    while counter4 <= 10:
        print(4*counter4)
        counter4 += 1
        
multiply1()
